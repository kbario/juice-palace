{
  "name": "@juice-palace/web",
  "description": "homepage for Juice Palace coffee van",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "tinacms": "tinacms",
    "tina.dev": "tinacms dev -c \"vite --mode ssr\"",
    "tina.build": "pnpm tinacms build",
    "build": "pnpm tina.build && pnpm build.qwik && pnpm postbuild",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.qwik": "qwik build",
    "build.server": "vite build -c adapters/deno/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\" && pnpm fmt.check",
    "lint.fix": "eslint \"src/**/*.ts*\" && pnpm fmt",
    "preview": "qwik build preview && vite preview --open",
    "serve": "deno run --allow-net --allow-read --allow-env server/entry.deno.js",
    "start": "vite --open --mode ssr",
    "postbuild": "mv ./public/admin/index.html ./dist/admin/index.html",
    "type-check": "tsc",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.3",
    "@builder.io/qwik-city": "^1.5.3",
    "@juice-palace/tailwind-config": "workspace:*",
    "@juice-palace/transactional": "workspace:*",
    "@qwik-ui/headless": "^0.3.8",
    "@qwik-ui/utils": "^0.2.1",
    "@qwikest/icons": "^0.0.13",
    "@tinacms/cli": "^1.5.45",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "qwik-ui": "^0.1.0",
    "tailwindcss": "3.3.3",
    "tinacms": "^1.6.3",
    "typescript": "5.3.3",
    "undici": "^6.15.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@modular-forms/qwik": "^0.24.0",
    "motion": "^10.17.0",
    "resend": "^3.2.0",
    "valibot": "^0.30.0"
  }
}
